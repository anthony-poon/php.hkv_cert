<style>
    @page {
        margin: 0;
        page-break-inside:avoid;
        background-position: center;
        background-repeat: no-repeat;
    }
    .text-wrapper {
        font-family: Cambria;
        padding-top: 4.5cm;
        text-align: center;
        font-size: 12pt;

    }
    .text-wrapper div {
        padding-bottom: 6pt;
        z-index: 2;
    }
    .pt-9{
        font-size: 9pt;

    }
    .pt-14{
        font-size: 14pt;

    }
    .pt-16{
        font-size: 16pt;

    }
    .pt-18{
        font-size: 18pt;

    }
    .pt-20{
        font-size: 20pt;

    }
    .pt-28{
        font-size: 28pt;

    }
    #cert-info{
        font-weight: bold;
    }
    #full-name {
        font-weight: bold;
        margin-top: 0.3cm;
        margin-bottom: 0.3cm;
    }
    #course-name {
        font-weight: bold;
        margin-top: 0.3cm;
        margin-bottom: 0.3cm;
    }
    #date {

    }
    #date div {
        padding: 0;
    }
    #logo {
        position: fixed;
        top: 24.1cm;
        left: 3cm;
        z-index: 1;
    }
    #signature {
        padding: -24px;
        z-index: 1;
    }
</style>
<div class="page-wrapper">
    <div class="text-wrapper">
        <div id="cert-info">
            <div class="pt-28">Certificate of Achievement</div>
            <div>CQI & IRCA UDN</div>
            <div>{{ doc_data["certno"] }}</div>
            <div>CQI & IRCA UDN: {{ doc_data["atp_delegate_id"] }}</div>
        </div>
        <div class="pt-18">This is to certify that</div>
        <div id="full-name" class="pt-28">{{ doc_data["first_name"] }} {{ doc_data["last_name"] }}</div>
        <div>has successfully completed a {{ doc_data["duration"] }}</div>
        <div id="course-name" class="pt-18">
            <div>ISO 45001:2018</div>
            <div>Occupational Health and Safety</div>
            <div>Management Systems</div>
            <div>Lead Auditor Course</div>
        </div>
        <div id="date" class="pt-14">
            <div>from</div>
            <div>{{ doc_data["dates"] }}</div>
            <div>in</div>
            <div>{{ doc_data["city"] }} {{ doc_data["country"] }}</div>
        </div>
        <img id="signature" src="data:image/png;base64,"/>
        <div>Authorized Signature</div>
        <div class="pt-14"><b>HKV Academy</b></div>
        <div>www.hkv.co.uk</div>
        <div class="pt-9">Course certified by the CQI & IRCA under ID {{ doc_data["course_id"] }}</div>
        <div class="pt-9">This certificate is valid for five (5) years for the purpose of auditor registration by the CQI & IRCA</div>
    </div>
</div>
<div id="logo">
    <img src="data:image/png;base64,"/>
</div>

